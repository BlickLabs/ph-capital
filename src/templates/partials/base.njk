<!DOCTYPE html>
<html>
  <head>
    {% include 'seo/google.njk' %}
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1">
    {% include 'seo/metatags.njk' %}
    {% block extra_css %}{% endblock %}
    <link rel="stylesheet" href="css/{{ projectFilesName }}.libs.css">
    <link rel="stylesheet" href="css/main.css">
    <title>{% block title %}{% endblock %} | PH Capital</title>
    {% include 'seo/facebook.njk' %}
  </head>
  <body>
    <div id="page">
      {% if lang_view == "english" %}
      {% include 'partials/navbar.njk' %}
      {% else %}
        {% include 'partials/navbar-es.njk' %}
      {% endif %}
      <main>
        {% block content %}{% endblock %}
      </main>
      {% if lang_view == "english" %}
        {% include 'partials/footer.njk' %}
      {% else %}
        {% include 'partials/footer-es.njk' %}
      {% endif %}
    </div>
    {% block js_libs_config %}
    {% endblock %}
    <script src="js/{{ projectFilesName }}.libs.js"></script>
    {% block js_project_config %}
    {% endblock %}
    <script src="js/{{ projectFilesName }}.js"></script>
    {% block js_inline %}
    {% endblock %}
  </body>
</html>
