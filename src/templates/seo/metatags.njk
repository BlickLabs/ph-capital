{% block meta %}
  <meta name="description" content="{{meta_description}}" />

  <meta property="og:url" content="{{og_url}}" />
  <meta property="og:type" content="website" />
  <meta property="og:title" content="{{og_title}}" />
  <meta property="og:description" content="{{og_description}}" />
  <meta property="og:image" content="{{og_img}}" />
  <meta property="og:image:width" content="{{og_img_width}}" />
  <meta property="og:image:height" content="{{og_img_height}}" />

  <meta name="title" content="We invest in the heart of the cities | PH Capital" />
  

  <link runat="server" rel="shortcut icon" href="img/favicon.png" type="image/x-icon"/>
  <link runat="server" rel="icon" href="img/favicon.png" type="image/ico"/>
{% endblock %}
